{
  "name": "chatgpt-linkedin-ui-extension",
  "displayName": "Danny ChatGPT extension",
  "version": "0.0.1",
  "description": "A basic Plasmo extension.",
  "author": "Kader Gomez",
  "scripts": {
    "start": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "clsx": "^1.2.1",
    "daisyui": "^2.51.5",
    "plasmo": "0.67.4",
    "prism-react-renderer": "^1.3.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.10.0",
    "scrapedin": "^1.0.21",
    "tailwind-merge": "^1.12.0",
    "theme-change": "^2.5.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.3",
    "@storybook/react": "^7.0.2",
    "@types/chrome": "0.0.210",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.21",
    "prettier": "2.8.3",
    "tailwindcss": "^3.3.1",
    "typescript": "4.9.4"
  },
  "manifest": {
    "author": "Danny HoldCamp",
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "static\\content.tsx"
        ],
        "run_at": "document_start"
      }
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "permissions": [
      "storage",
      "scripting",
      "commands",
      "tabs"
    ]
  }
}
